<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="TransparentStatusBar" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowTranslucentNavigation">true</item>
    </style>


    <style name="EditTextStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_marginStart">32dp</item>
        <item name="android:layout_marginEnd">32dp</item>
        <item name="android:layout_marginTop">16dp</item>

        <item name="android:gravity">center</item>
        <item name="android:maxLines">1</item>
        <item name="android:textColorHint">@color/hintColor</item>

        <item name="android:textColor">#ff9eaabe</item>
        <item name="android:textSize">12sp</item>

        <item name="android:background">@drawable/et_</item>
    </style>

    <style name="victor_nic">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:textSize">24sp</item>
        <item name="android:letterSpacing">-0.24</item>
    </style>

    <style name="vip">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:textSize">10sp</item>
        <item name="android:letterSpacing">0.31</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="ProfileText">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:textSize">19sp</item>
        <item name="android:letterSpacing">0.57</item>
    </style>

    <style name="EditTextStyle0">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">50dp</item>

        <item name="android:layout_marginTop">8dp</item>

        <item name="android:gravity">center</item>
        <item name="android:maxLines">1</item>
        <item name="android:textColorHint">@color/hintColor</item>

        <item name="android:textColor">#ff9eaabe</item>
        <item name="android:textSize">12sp</item>

        <item name="android:background">@drawable/et_</item>
    </style>


    <style name="title">
